jQuery(function($){
	// AJAX form submission	function change_design(button) {
		$.ajax({
			type: 'POST',
			url: home_url+'/?change_theme='+button,
			data: {
				'ptc_nonce' : $("#ptc_nonce").val(),
			},
			success: function(data, textStatus) {
				$( "#wppb-page-reload" ).dialog({width:250,minWidth:250,maxWidth:250,modal:true,autoOpen:true,});
				$(location).attr('href',home_url);
			},
			error: function(jqXHR, textStatus, errorThrown) {
				$( "#wppb-external-connection-failure" ).dialog({width:250,minWidth:250,maxWidth:250,modal:true,autoOpen:true,});
			},
			dataType: 'html'
		});
 
	}$('#myformwppaintbrush-magazine').click(function() {change_design( 'wppaintbrush-magazine' );});$('#myformcoraline').click(function() {change_design( 'coraline' );});$('#myform2011').click(function() {change_design( '2011' );});$('#myformenterprize').click(function() {change_design( 'enterprize' );});
